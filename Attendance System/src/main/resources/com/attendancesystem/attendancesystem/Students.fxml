<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>

<VBox xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.attendancesystem.attendancesystem.StudentsController"
      prefWidth="1200" prefHeight="800" spacing="15">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <HBox>
        <Region HBox.hgrow="ALWAYS"/>
        <Button text="Main Menu" onAction="#goBackToMainScene"/>
    </HBox>
    <Label text="Students List" style="-fx-font-weight: bold;"/>
    <Label fx:id="dataLabel"/>
</VBox>
